var current_search_call;function ajax_search(a){var e=$("#"+a),n=e.find(".search-results"),r=e.find(".search-form"),c=r.find(".search-input");$("html").click(function(){n.hide()});r.find(".search-submit");c.on("input",function(){var a=c.val();a.length>0&&(n.show(),(current_search_call=$.ajax({type:"POST",url:window.my_ajaxurl,data:{action:"ajax_search",keyword:a},dataType:"text"})).done(function(a){a.length>0?n.html(a):n.hide()}))})}jQuery(document).ready(function(a){ajax_search("header-search")});